{
  "extends": "google",
  "env": {
    "commonjs": true,
    "es6": true
  },
  "rules": {
    "no-restricted-syntax": "off",
    "linebreak-style": [
      "error",
      "windows"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "curly": "error",
    "max-len": [
      "error",
      120
    ]
  }
}